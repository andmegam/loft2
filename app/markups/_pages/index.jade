extends ../main


block title 
	| Каталог мобильныех телефонов

block content_main 
	main.main
		section.view
			div.sorting Сортировать по
				select.sorting-list
					option.sorting-item(value="sort-mony") по цене
					option.sorting-item(value="sort-rating") по популярности
			div.viewcatalog Вид каталога
			ul.viewcatalog__list
				li.viewcatalog__item.activ
					a.viewcatalog__link.viewtable(href="#" id="viewtable" title="Таблица") viewtable
				li.viewcatalog__item
					a.viewcatalog__link.viewcontent(href="#" id="viewcontent" title="Содержимое") viewcontent
				li.viewcatalog__item
					a.viewcatalog__link.viewlist(href="#" id="viewlist" title="Список") viewlist
		section.products.viewtable
			ul.products__list
				+product(samsung, path)
				+product(lumia640, path)
				+product(archos, path)
				+product(alcatel, path)
				+product(fly, path)
				+product(highscreen, path)
				
		div.page-nav
			ul.page-nav__list
				li.page-nav__item
					a.page-nav__link(href="#") &lt;
				li.page-nav__item
					a.page-nav__link(href="#") 1
				li.page-nav__item
					a.page-nav__link.activ(href="#") 2
				li.page-nav__item
					a.page-nav__link(href="#") 3
				li.page-nav__item
					a.page-nav__link(href="#") &gt;

